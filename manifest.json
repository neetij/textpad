{
	"manifest_version" : 2,
	"name": "textpad",
	"version": "0.1",
	"description": "Write anything in a tab and save it",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"chrome_url_overrides" : {
		"newtab" : "textedit.html"
	},
	"content_security_policy": "script-src 'self' https://www.google.com; object-src 'self';",
	"permissions": [
		"https://*.google.com/"
	]
} 
